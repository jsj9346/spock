[run]
source = modules
branch = True
omit =
    */tests/*
    */__pycache__/*
    */venv/*
    */anaconda3/*
    spock_init_db.py

[report]
precision = 2
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:

[html]
directory = htmlcov

[json]
output = coverage.json
